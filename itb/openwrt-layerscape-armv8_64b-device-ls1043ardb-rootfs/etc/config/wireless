
config wifi-device 'radio0'
	option type 'mac80211'
	option channel '12'
	option hwmode 'g'
	option beacon_int '100'
	option dtim_period '2'
	option wmm '1'
	option ieee80211n '1'
#	option ieee80211ac '0'
	option path 'soc/3600000.pcie/pci0000:00/0000:00:00.0/0000:01:00.0'	
	option disabled '0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'OpenWrt'
	option encryption 'psk2'
	option key '123456789'


